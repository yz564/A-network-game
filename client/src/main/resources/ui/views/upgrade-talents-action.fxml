            <font>
                <Font name="Open Sans Regular" size="7.0"/>
            </font>
        </Label>
        <ImageView fx:id="newTalentImage" fitHeight="36.0" pickOnBounds="true" preserveRatio="true"
                   GridPane.columnIndex="7" GridPane.halignment="LEFT" GridPane.rowIndex="7">
            <image>
                <Image url="@../icons/tooltip/level1.png"/>
            </image>
        </ImageView>
        <Label fx:id="newTalentLabel" alignment="CENTER" contentDisplay="CENTER" maxHeight="-Infinity"
               maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="10.0" prefWidth="45.0"
               text="Master" textAlignment="CENTER" textFill="WHITE" GridPane.columnIndex="7" GridPane.rowIndex="8">
            <font>
                <Font name="Open Sans Regular" size="7.0"/>
            </font>
        </Label>
        <Label alignment="CENTER" contentDisplay="CENTER" maxHeight="-Infinity" maxWidth="-Infinity"
               minHeight="-Infinity" minWidth="-Infinity" prefHeight="15.0" prefWidth="80.0" text="To Upgrade"
               textAlignment="CENTER" textFill="WHITE" GridPane.columnIndex="4" GridPane.rowIndex="6">
            <font>
                <Font name="Open Sans Regular" size="10.0"/>
            </font>
        </Label>
        <Label fx:id="oldTalentNum" alignment="CENTER" contentDisplay="CENTER" maxHeight="-Infinity" maxWidth="-Infinity"
               minHeight="-Infinity" minWidth="-Infinity" prefHeight="40.0" prefWidth="50.0" text="100"
               textAlignment="CENTER" textFill="WHITE" GridPane.columnIndex="2" GridPane.rowIndex="7">
            <font>
                <Font name="Open Sans Regular" size="20.0"/>
            </font>
        </Label>
        <Label fx:id="newTalentNum" alignment="CENTER" contentDisplay="CENTER" maxHeight="-Infinity" maxWidth="-Infinity"
               minHeight="-Infinity" minWidth="-Infinity" prefHeight="40.0" prefWidth="50.0" text="100"
               textAlignment="CENTER" textFill="WHITE" GridPane.columnIndex="6" GridPane.rowIndex="7">
            <font>
                <Font name="Open Sans Regular" size="20.0"/>
            </font>
        </Label>
        <TextField fx:id="upgradeNum" alignment="CENTER" maxHeight="-Infinity" maxWidth="-Infinity"
                   minHeight="-Infinity" minWidth="-Infinity" prefHeight="26.0" prefWidth="50.0" text="0"
                   GridPane.columnIndex="4" GridPane.rowIndex="7">
            <font>
                <Font name="Open Sans Regular" size="13.0"/>
            </font>
        </TextField>
        <Slider fx:id="slider" majorTickUnit="5.0" minorTickCount="4" snapToTicks="true" GridPane.columnIndex="3"
                GridPane.columnSpan="3" GridPane.rowIndex="8"/>
