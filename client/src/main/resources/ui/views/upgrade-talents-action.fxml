               minHeight="-Infinity" minWidth="-Infinity" prefHeight="45.0" prefWidth="300.0" text="Upgrade Talents"
               textAlignment="CENTER" textFill="WHITE" GridPane.columnSpan="9" GridPane.rowIndex="0"
               GridPane.valignment="BOTTOM">
            <font>
                <Font name="EB Garamond Regular" size="35.0"/>
            </font>
        </Label>
        <Label alignment="CENTER" contentDisplay="CENTER" maxHeight="-Infinity" maxWidth="-Infinity"
               minHeight="-Infinity" minWidth="-Infinity" prefHeight="15.0" prefWidth="80.0" text="Upgrade From"
               textAlignment="CENTER" textFill="WHITE" GridPane.columnIndex="1" GridPane.columnSpan="3"
               GridPane.rowIndex="4">
            <font>
                <Font name="Open Sans Regular" size="10.0"/>
            </font>
        </Label>
        <Label alignment="CENTER" contentDisplay="CENTER" maxHeight="-Infinity" maxWidth="-Infinity"
               minHeight="-Infinity" minWidth="-Infinity" prefHeight="15.0" prefWidth="80.0" text="Upgrade To"
               textAlignment="CENTER" textFill="WHITE" GridPane.columnIndex="5" GridPane.columnSpan="3"
               GridPane.rowIndex="4">
            <font>
                <Font name="Open Sans Regular" size="10.0"/>
            </font>
        </Label>
        <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0" GridPane.columnIndex="1" GridPane.columnSpan="7"
              GridPane.rowIndex="9">
            <children>
                <Label alignment="CENTER" contentDisplay="CENTER" maxHeight="-Infinity" maxWidth="-Infinity"
                       minHeight="-Infinity" minWidth="-Infinity" prefHeight="40.0" prefWidth="80.0" text="Total Cost:"
                       textAlignment="CENTER" textFill="WHITE">
                    <font>
                        <Font name="Open Sans Regular" size="13.0"/>
                    </font>
                </Label>
                <Label fx:id="actionCost" alignment="CENTER" contentDisplay="CENTER" maxHeight="-Infinity"
                       maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="40.0"
                       prefWidth="50.0" text="0" textAlignment="CENTER" textFill="WHITE">
                    <font>
                        <Font name="Open Sans Regular" size="20.0"/>
                    </font>
                </Label>
                <Label alignment="CENTER" contentDisplay="CENTER" maxHeight="-Infinity" maxWidth="-Infinity"
                       minHeight="-Infinity" minWidth="-Infinity" prefHeight="40.0" prefWidth="10.0" text="/"
                       textAlignment="CENTER" textFill="WHITE">
                    <font>
                        <Font name="Open Sans Regular" size="20.0"/>
                    </font>
                </Label>
                <Label fx:id="resourceAvailable" alignment="CENTER" contentDisplay="CENTER" maxHeight="-Infinity"
                       maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="40.0"
                       prefWidth="50.0" text="100" textAlignment="CENTER" textFill="WHITE">
                    <font>
                        <Font name="Open Sans Regular" size="20.0"/>
                    </font>
                </Label>
            </children>
        </HBox>
        <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0" spacing="50.0" GridPane.columnIndex="1"
              GridPane.columnSpan="7" GridPane.rowIndex="10">
            <children>
                <Button alignment="CENTER" contentDisplay="CENTER" maxHeight="-Infinity" maxWidth="-Infinity"
                        minHeight="-Infinity" minWidth="-Infinity" mnemonicParsing="false" onAction="#onAction"
                        prefHeight="30.0" prefWidth="70.0" text="Confirm" textAlignment="CENTER"/>
                <Button alignment="CENTER" contentDisplay="CENTER" maxHeight="-Infinity" maxWidth="-Infinity"
                        minHeight="-Infinity" minWidth="-Infinity" mnemonicParsing="false" onAction="#onCancel"
                        prefHeight="30.0" prefWidth="70.0" text="Cancel" textAlignment="CENTER"/>
            </children>
        </HBox>
        <Label fx:id="errorMessage" alignment="CENTER" contentDisplay="CENTER" maxHeight="-Infinity"
               maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="50.0" prefWidth="241.0"
               text="" textAlignment="CENTER" textFill="HOTPINK" wrapText="true" GridPane.columnSpan="9"
               GridPane.rowIndex="11">
            <font>
                <Font name="Open Sans Regular" size="10.0"/>
            </font>
        </Label>
    </children>
    <fx:define>
        <ArrayList fx:id="sliderList">
            <fx:reference source="slider"/>
        </ArrayList>
    </fx:define>
    <fx:define>
        <ArrayList fx:id="numList">
            <fx:reference source="upgradeNum"/>
        </ArrayList>
    </fx:define>
</GridPane>
