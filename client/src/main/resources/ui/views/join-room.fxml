      <RowConstraints minHeight="10.0" percentHeight="14.0" prefHeight="30.0" vgrow="SOMETIMES" />
  </rowConstraints>
   <children>
      <Label fx:id="header" alignment="CENTER" prefHeight="17.0" prefWidth="615.0" text="Join Game" GridPane.columnSpan="3">
