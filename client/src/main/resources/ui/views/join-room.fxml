      </Label>
      <Button fx:id="joinRoomOneButton" alignment="CENTER" mnemonicParsing="false" onAction="#onJoinRoomOne" prefHeight="26.0" prefWidth="143.0" text="Join" GridPane.columnIndex="2" GridPane.rowIndex="2">
         <GridPane.margin>
            <Insets left="5.0" right="20.0" />
         </GridPane.margin>
      </Button>
      <Button fx:id="joinRoomTwoButton" alignment="CENTER" mnemonicParsing="false" onAction="#onJoinRoomTwo" prefHeight="26.0" prefWidth="143.0" text="Join" GridPane.columnIndex="2" GridPane.rowIndex="3">
