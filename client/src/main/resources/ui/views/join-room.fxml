            <Insets left="5.0" right="20.0" />
         </GridPane.margin>
      </Button>
      <Button fx:id="joinRoomFourButton" alignment="CENTER" mnemonicParsing="false" onAction="#onJoinRoomFour" prefHeight="26.0" prefWidth="143.0" text="Join" GridPane.columnIndex="2" GridPane.rowIndex="5">
         <GridPane.margin>
            <Insets left="5.0" right="20.0" />
         </GridPane.margin>
      </Button>
      <Label fx:id="joinRoomErrorLabel" alignment="CENTER" prefHeight="17.0" prefWidth="338.0" textFill="#fd0808" GridPane.columnSpan="3" GridPane.rowIndex="6">
