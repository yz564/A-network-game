          xmlns="http://javafx.com/javafx/16"
          fx:controller="edu.duke.ece651.risk.client.controller.JoinRoomController">
  <columnConstraints>
    <ColumnConstraints hgrow="SOMETIMES" maxWidth="350.0" minWidth="10.0" percentWidth="25.0" prefWidth="303.0" />
    <ColumnConstraints hgrow="SOMETIMES" maxWidth="297.0" minWidth="10.0" percentWidth="45.0" prefWidth="297.0" />
